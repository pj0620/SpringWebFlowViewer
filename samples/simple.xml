<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/webflow
		https://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	start-state="start">

	<view-state id="start">
		<transition on="a" to="A1"/>
		<transition on="b" to="B1"/>
		<transition on="c" to="C1"/>
	</view-state>

    <view-state id="A1">
		<transition on="a" to="A2"/>
	</view-state>

    <view-state id="A2">
<!--		<transition on="a" to="A1"/>-->
<!--		<transition on="d" to="start"/>-->
		<transition on="a" to="finish"/>
	</view-state>

    <view-state id="B1">
		<transition on="b" to="B2"/>
	</view-state>

    <view-state id="B2">
		<transition on="b" to="finish"/>
	</view-state>

    <view-state id="C1">
		<transition on="c" to="C2"/>
	</view-state>

    <view-state id="C2">
		<transition on="c" to="finish"/>
	</view-state>

	<end-state id="finish" />

</flow>